@model MonitoriaMVC.Models.Cliente

@{
    ViewData["Title"] = "Cadastro";
}

<h1>Cadastro</h1>

<form asp-action="Index" method="post">
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" aria-describedby="nome" asp-for="Nome">
    </div>
    <div class="form-group">
        <label for="email">Email</label>
        <input type="email" class="form-control" aria-describedby="email" asp-for="Email">
    </div>
    <div class="form-group">
        <label for="senha">Senha</label>
        <input type="password" class="form-control" asp-for="Senha">
    </div>
    <button type="submit" class="btn btn-primary">Cadastrar</button>
</form>

